{
  "name": "vue_demo",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently vite \"pnpm electron:dev\"",
    "electron:dev": "pnpm build:main && electron .",
    "build": "pnpm build:render && pnpm build:main && electron-builder",
    "build:preview": "pnpm build:render && pnpm build:main && pnpm build:package",
    "build:render": "vue-tsc --noEmit && vite build",
    "build:main": "tsc -p src/main --outDir dist/main",
    "package": "electron-builder --dir",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.2.9",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.5",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "build": {
    "appId": "klasjdlaksjdkljdqkljwdasd",
    "directories": {
      "output": "out"
    }
  }
}